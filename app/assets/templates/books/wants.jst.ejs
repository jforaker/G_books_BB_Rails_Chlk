
<!-- Modal -->
<div class="modal fade" id="wantsModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h2> items you want </h2>
      </div>
      <div class="modal-body">

            <div class="tile">
                <img class="" src="<%= image %>">

                <h4 class="tile-title"><%= title %></h4>

                    <div class="tagsinput">
                    <span class="tag">
                        <span>Remove &nbsp;</span>
                        <a class="remove-item tagsinput-remove-link"></a>
                    </span>
                    </div>

            </div>

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->