
<!-- Modal -->
<div class="modal fade" id="deetsModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h2> <%= title %> </h2>
      </div>
      <div class="modal-body">
        <div id="viewerStatus" style="padding: 5px; background-color: rgb(238, 238, 238); display: none;"></div>
        <div id="viewerCanvas"></div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        <button id="want-button" type="button" class="btn btn-primary">Want it</button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal-dialog -->
</div><!-- /.modal -->